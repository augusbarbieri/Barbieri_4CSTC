ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `mydb`.`Fuente`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `mydb`.`Fuente` (
  `idFuente` INT NOT NULL AUTO_INCREMENT,
  `descripcion` VARCHAR(45) NULL,
  `marca` VARCHAR(45) NULL,
  `modelo` VARCHAR(45) NULL,
  `precio` DOUBLE NULL,
